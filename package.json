{
  "name": "devide",
  "displayName": "dev.IDE",
  "description": "placeholder",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
